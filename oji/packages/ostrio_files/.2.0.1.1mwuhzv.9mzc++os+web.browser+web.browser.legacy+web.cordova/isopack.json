{
  "isopack-2": {
    "name": "ostrio:files",
    "summary": "Upload files to Meteor application, with 3rd party storage support: AWS:S3, GridFS and other",
    "version": "2.0.1",
    "isTest": false,
    "builds": [
      {
        "kind": "main",
        "arch": "os",
        "path": "os.json"
      },
      {
        "kind": "main",
        "arch": "web.browser",
        "path": "web.browser.json"
      },
      {
        "kind": "main",
        "arch": "web.browser.legacy",
        "path": "web.browser.legacy.json"
      },
      {
        "kind": "main",
        "arch": "web.cordova",
        "path": "web.cordova.json"
      }
    ],
    "plugins": []
  }
}