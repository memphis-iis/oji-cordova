{
  "isopack-2": {
    "name": "activitree:push",
    "summary": "Push Notifications for Cordova and Web/PWA with Firebase (FCM).",
    "version": "2.0.10",
    "isTest": false,
    "builds": [
      {
        "kind": "main",
        "arch": "os",
        "path": "os.json"
      },
      {
        "kind": "main",
        "arch": "web.browser",
        "path": "web.browser.json"
      },
      {
        "kind": "main",
        "arch": "web.browser.legacy",
        "path": "web.browser.legacy.json"
      },
      {
        "kind": "main",
        "arch": "web.cordova",
        "path": "web.cordova.json"
      }
    ],
    "plugins": [],
    "cordovaDependencies": {
      "phonegap-plugin-push": "2.3.0",
      "cordova-plugin-device": "2.0.3"
    }
  }
}